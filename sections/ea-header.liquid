<svg xmlns="http://www.w3.org/2000/svg" class="hidden">
  <symbol id="icon-search" width="30.019" height="30.075" viewBox="0 0 30.019 30.075">
    <path d="M23.132 21.359a1.236 1.236 0 0 1-1.749 1.747l-6.049-6.04a.766.766 0 0 0-.937-.087 5.517 5.517 0 0 1-.527.333 9.262 9.262 0 1 1 3.448-3.432 4.859 4.859 0 0 1-.314.492.763.763 0 0 0 .082.943zM9.273 16.06a6.795 6.795 0 1 0-6.8-6.792 6.8 6.8 0 0 0 6.8 6.792z" transform="rotate(14.98 1.022 25.789)" style="fill:#fff;stroke:#253746" data-name="search-icon"/>
  </symbol>

  <symbol id="icon-close" class="icon icon-close" fill="none" viewBox="0 0 18 17">
    <path d="M.865 15.978a.5.5 0 00.707.707l7.433-7.431 7.579 7.282a.501.501 0 00.846-.37.5.5 0 00-.153-.351L9.712 8.546l7.417-7.416a.5.5 0 10-.707-.708L8.991 7.853 1.413.573a.5.5 0 10-.693.72l7.563 7.268-7.418 7.417z" fill="currentColor">
  </symbol>
</svg>
<{% if section.settings.enable_sticky_header %}sticky-header{% else %}div{% endif %} class="header-wrapper {% if section.settings.show_line_separator %} header-wrapper--border-bottom{% endif %}">
  <header id="Header-{{ section.id }}" class="header header--{{ section.settings.logo_position }} page-width{% if section.settings.menu != blank %} header--has-menu{% endif %}">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent" role="link">
      {{ "accessibility.skip_to_text" | t }}
    </a>
    {%- if section.settings.mobile_menu != blank -%}
      <header-drawer data-breakpoint="tablet">
        <details id="Details-menu-drawer-container" class="menu-drawer-container">
          <summary class="header__icon header__icon--menu header__icon--summary link focus-inset" aria-label="{{ 'sections.header.menu' | t }}">
            <span>
              {% render 'icon-hamburger-alt' %}
            </span>
          </summary>
          <div id="menu-drawer" class="gradient menu-drawer motion-reduce" tabindex="-1">
            <div class="menu-drawer-heading" >
              <div class="menu-drawer-close">
                {% render 'icon-close' %} 
                Close Menu
              </div>
              <div class="menu-drawer-logo">
                {%- if section.settings.svg_logo_mobile != blank -%}
                  <div class="mobile-logo svg medium-hide large-up-hide" aria-label="{{ shop.name }}">
                    {{ section.settings.svg_logo_mobile }}
                  </div>
                {%- else -%}
                  {%- if section.settings.logo_mobile != blank -%}
                    {%- assign image_size_2x_mobile = section.settings.logo_width | times: 2 | at_most: 5760 -%}
                    <img class="mobile-logo large-up-hide" srcset="{{ section.settings.logo_mobile | image_url: width: section.settings.logo_width }} 1x, {{ section.settings.logo_mobile | image_url: width: image_size_2x_mobile }} 2x"
                      src="{{ section.settings.logo_mobile | image_url: width: section.settings.logo_width }}"
                      loading="lazy"
                      class="header__heading-logo"
                      width="{{ section.settings.logo_mobile.width }}"
                      height="{{ section.settings.logo_mobile.height }}"
                      alt="{{ section.settings.logo_mobile.alt | default: shop.name | escape }}"
                    >
                  {%- else -%}
                    <span class="h2">{{ shop.name }}</span>
                  {%- endif -%}
                {%- endif -%}
              </div>
            </div>
            <div class="menu-drawer__inner-container">
              <div class="menu-drawer__navigation-container">
                <nav class="menu-drawer__navigation">
                  {%- comment -%}Removed class has-submenu {%- endcomment -%}
                  <ul class="menu-drawer__menu list-menu" role="list">
                    {%- for link in section.settings.mobile_menu.links -%}
                      {%- if link.links != blank -%}
                        {%- for childlink in link.links -%}
                        {%- liquid
                          assign menu_block = null
                          for block in section.blocks
                            case block.type
                              when 'mega_menu'
                              if block.settings.parent == childlink.title
                                assign menu_block = block
                              endif
                            endcase                                      
                          endfor
                        -%}
                          <li>
                            {%- if childlink.links == blank -%}
                              {%- if menu_block != blank -%}
                                <input id="Details-menu-drawer-submenu-{{ forloop.index }}" {% if forloop.first %}checked{% endif %} type="radio" class="hidden" name="menudrawer--accordion">
                                <label for="Details-menu-drawer-submenu-{{ forloop.index }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset">
                                  {{ childlink.title | escape }}
                                  {% render 'icon-caret' %}
                                </label>
                                <div class="mega-menu__sublist_cards">
                                  
                                  {% for i in (1..3) %}
                                    {%- capture menu_product -%}product__{{ i }}{%- endcapture -%}
                                    {%- capture menu_image -%}image__{{ i }}{%- endcapture -%}
                                    {%- capture menu_title -%}title__{{ i }}{%- endcapture -%}
                                    {%- capture menu_link -%}link__{{ i }}{%- endcapture -%}
      
                                    {%- liquid
                                    if menu_block.settings[menu_product] != blank 
                                      assign product = menu_block.settings[menu_product] 
                                      assign badge = product.metafields.eag_fields.product_badge 
                                      render 'mega-menu', isProduct: true, image: product.featured_image, title: product.title , link: product.url, badge: badge 
                                    elsif menu_block.settings[menu_image] != blank or menu_block.settings[menu_title] != blank or menu_block.settings[menu_link] != blank
                                      render 'mega-menu', isProduct: false, image: menu_block.settings[menu_image], title: menu_block.settings[menu_title] , link: menu_block.settings[menu_link] 
                                    endif 
                                    -%}
                                  {% endfor %}
                                </div>
                              {%- else -%}
                              <a href="{{ childlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if childlink.current %} menu-drawer__menu-item--active{% endif %}"{% if childlink.current %} aria-current="page"{% endif %}>
                                {{ childlink.title | escape }}
                              </a>
                              {%- endif -%}
                            {%- else -%}
                              <details id="Details-menu-drawer-submenu-{{ forloop.index }}">
                                <summary class="menu-drawer__menu-item link link--text list-menu__item focus-inset">
                                  {{ childlink.title | escape }}
                                  {% render 'icon-arrow' %}
                                  {% render 'icon-caret' %}
                                </summary>
                                {%- comment -%} REmoved class has-submenu{%- endcomment -%}
                                <div id="childlink-{{ childlink.title | escape }}" class="menu-drawer__submenu  gradient motion-reduce">
                                  <button class="menu-drawer__close-button link link--text focus-inset" aria-expanded="true">
                                    {% render 'icon-arrow' %}
                                    {{ childlink.title | escape }}
                                  </button>
                                  <ul class="menu-drawer__menu list-menu" role="list" tabindex="-1">
                                    {%- for grandchildlink in childlink.links -%}
                                      <li>
                                        <a href="{{ grandchildlink.url }}" class="menu-drawer__menu-item link link--text list-menu__item focus-inset{% if grandchildlink.current %} menu-drawer__menu-item--active{% endif %}"{% if grandchildlink.current %} aria-current="page"{% endif %}>
                                          {{ grandchildlink.title | escape }}
                                        </a>
                                      </li>
                                    {%- endfor -%}
                                  </ul>
                                </div>
                              </details>
                            {%- endif -%}
                          </li>
                        {%- endfor -%}
                      {%- else -%}
                        <li>
                          <a href="{{ link.url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset{% if link.current %} menu-drawer__menu-item--active{% endif %}"{% if link.current %} aria-current="page"{% endif %}>
                            <div class="menu-drawer__menu-item-inner">
                            {% if mobile_menu_svg_icon != blank %}
                              <div class="mobile_menu_svg_icon">
                                {{ mobile_menu_svg_icon }}
                              </div>
                              {% else %}
                                {% if mobile_menu_svg_icon != blank %}
                                <div class="mobile_menu_svg_image">
                                  <img srcset="{{ mobile_menu_svg_image | image_url: width: 40 }} 1x, {{ mobile_menu_svg_image | image_url: width: 80 }} 2x"
                                    src="{{ mobile_menu_svg_image | image_url: width: 40 }}"
                                    loading="lazy"
                                    class="header__heading-logo"
                                    width="{{ mobile_menu_svg_image.width }}"
                                    height="{{ mobile_menu_svg_image.height }}"
                                    alt="{{ mobile_menu_svg_image.alt | default: shop.name | escape }}"
                                  >
                                </div>
                                {% endif %}
                            {% endif %}
                            {{ link.title | escape }}
                            </div>
                          </a>
                        </li>
                      {%- endif -%}
                    {%- endfor -%}
                    {%- if shop.customer_accounts_enabled -%}
                      <li>
                        {%- if customer -%}
                          <a href="{{ routes.account_url }}" class="menu-drawer__menu-item list-menu__item link link--text focus-inset">
                            <div class="menu-drawer__menu-item-inner">
                                {{ 'customer.account_fallback' | t }}
                            </div>
                          </a>
                        {%- else -%}
                          <modal-opener data-modal="#accounts">
                            <button data-href="{{ routes.account_login_url }}" aria-haspopup="dialog" class="menu-drawer__menu-item list-menu__item link link--text focus-inset" 
                              aria-label="{{ 'customer.log_in' | t }}">
                              <div class="menu-drawer__menu-item-inner">
                                {{ 'customer.log_in' | t }}
                              </div>
                            </button>
                          </modal-opener>
                        {%- endif -%}
                      </li>
                    {%- endif -%}
                  </ul>
                </nav>
                
                {%- if section.settings.header_button_text != blank and section.settings.header_button_url != blank -%}
                  <a href="{{ section.settings.header_button_url }}" class="button header_button">{{ section.settings.header_button_text }}</a>
                {%- endif -%}
                <div class="menu-drawer__featured-items-wrap">
                  {%- if section.settings.images_title != blank -%}
                  <span class="menu-drawer__featured_title">
                    {{ section.settings.images_title }}
                  </span>
                  {%- endif -%}
                  {%- comment -%}
                  <div class="menu-drawer__featured-items">
                  {% for i in (1..3) %}
                    {%- capture featured_product -%}product__{{ i }}{%- endcapture -%}
                    {%- capture featured_image -%}image__{{ i }}{%- endcapture -%}
                    {%- capture featured_title -%}title__{{ i }}{%- endcapture -%}
                    {%- capture featured_link -%}link__{{ i }}{%- endcapture -%}

                    {%- liquid
                    if section.settings[featured_product] != blank 
                      assign product = section.settings[featured_product] 
                      assign badge = product.metafields.eag_fields.product_badge 
                      render 'mega-menu', isProduct: true, image: product.featured_image, title: product.title , link: product.url, badge: badge 
                    else 
                      render 'mega-menu', isProduct: false, image: section.settings[featured_image], title: section.settings[featured_title] , link: section.settings[featured_link] 
                    endif 
                    -%}
                  {% endfor %}
                  </div>
                  {%- endcomment -%}
                </div>
                <div class="menu-drawer__utility-links">
                  <ul class="list list-social list-unstyled" role="list">
                    {%- if settings.social_twitter_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_twitter_link }}" class="list-social__link link">
                          {%- render 'icon-twitter' -%}
                          <span class="visually-hidden">{{ 'general.social.links.twitter' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_facebook_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_facebook_link }}" class="list-social__link link">
                          {%- render 'icon-facebook' -%}
                          <span class="visually-hidden">{{ 'general.social.links.facebook' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_pinterest_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_pinterest_link }}" class="list-social__link link">
                          {%- render 'icon-pinterest' -%}
                          <span class="visually-hidden">{{ 'general.social.links.pinterest' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_instagram_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_instagram_link }}" class="list-social__link link">
                          {%- render 'icon-instagram' -%}
                          <span class="visually-hidden">{{ 'general.social.links.instagram' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_tiktok_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_tiktok_link }}" class="list-social__link link">
                          {%- render 'icon-tiktok' -%}
                          <span class="visually-hidden">{{ 'general.social.links.tiktok' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_tumblr_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_tumblr_link }}" class="list-social__link link">
                          {%- render 'icon-tumblr' -%}
                          <span class="visually-hidden">{{ 'general.social.links.tumblr' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_snapchat_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_snapchat_link }}" class="list-social__link link">
                          {%- render 'icon-snapchat' -%}
                          <span class="visually-hidden">{{ 'general.social.links.snapchat' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_youtube_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_youtube_link }}" class="list-social__link link">
                          {%- render 'icon-youtube' -%}
                          <span class="visually-hidden">{{ 'general.social.links.youtube' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                    {%- if settings.social_vimeo_link != blank -%}
                      <li class="list-social__item">
                        <a href="{{ settings.social_vimeo_link }}" class="list-social__link link">
                          {%- render 'icon-vimeo' -%}
                          <span class="visually-hidden">{{ 'general.social.links.vimeo' | t }}</span>
                        </a>
                      </li>
                    {%- endif -%}
                  </ul>
                </div>
              </div>
            </div>
          </div>
        </details>
      </header-drawer>
    {%- endif -%}

    {%- if section.settings.enable_search -%}
      {%- if section.settings.logo_position == 'top-center' or section.settings.menu == blank -%}
        <details-modal class="header__search">
          <details>
            <summary class="header__icon header__icon--search header__icon--summary link focus-inset modal__toggle" aria-haspopup="dialog" aria-label="{{ 'general.search.search' | t }}">
              <span>
                <svg class="modal__toggle-open icon icon-search" aria-hidden="true" focusable="false" role="img">
                  <use href="#icon-search">
                </svg>
