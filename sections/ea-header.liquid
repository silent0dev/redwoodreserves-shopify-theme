<style>
  header-drawer {
    justify-self: start;
    margin-left: -2.2rem;
  }

  .header__inline-menu {
      display: none;
  }

  #MainContent {
      margin-top: 0;
  }

  @media screen and (min-width: 1200px) {
    header-drawer {
      display: none;
    }
    
    .header__inline-menu {
      display: block;
    }
    body:not(.product) #MainContent {
      margin-top: 4.5rem;
    }
  }

  .menu-drawer-container {
    display: flex;
    margin-left: 2rem;
  }

  .list-menu {
    list-style: none;
    padding: 0;
    margin: 0;
  }

  .list-menu--inline {
    display: inline-flex;
    flex-wrap: wrap;
  }

  .mega-menu__list li {
    padding-bottom: 0; /* 1.1 */
    display: flex;
    align-items: end;
  }

  .list-menu--inline li:last-of-type {
    padding-right: 0;
  }

  .list-menu--inline li a ,
  .list-menu--inline li span {
    font-size: 1.9rem;
    font-weight: bold;
    font-stretch: normal;
    font-style: normal;
    line-height: 1.37;
    letter-spacing: normal;
    text-align: left;
  }

  .list-menu__item {
    display: flex;
    align-items: center;
    line-height: calc(1 + 0.3 / var(--font-body-scale));
  }

  .list-menu__item--link {
    text-decoration: none;
    padding-bottom: 1rem;
    padding-top: 1rem;
    line-height: calc(1 + 0.8 / var(--font-body-scale));
  }

  @media screen and (min-width: 750px) {
    .list-menu__item--link {
      padding-bottom: 0.5rem;
      padding-top: 0.5rem;
    }
  }
</style>

{%- style -%}
  #Header-{{ section.id }},
  .header-wrapper,
  #Header-{{ section.id }} .mega-menu .mega-menu__content {
    background: {{ section.settings.color_background }};
    color: {{ section.settings.text_color }};
  }
  #Header-{{ section.id }} .header__icon--menu,
  #Header-{{ section.id }} li a,
  #Header-{{ section.id }} .header__menu-item,
  #Header-{{ section.id }} .header__menu-item span,
  #Header-{{ section.id }} .header__icon  .icon-text  {
    color: {{ section.settings.text_color }};
    text-decoration: none;
  }
  #Header-{{ section.id }} .header__menu-item:hover,
  #Header-{{ section.id }} .header__menu-item:hover span,
  #Header-{{ section.id }} .header__menu-item span.active,
  #Header-{{ section.id }} li:hover > a,
  #Header-{{ section.id }} li:hover > summary > span,
  #Header-{{ section.id }} li.header__active-menu-item  span,
  #Header-{{ section.id }} .list-menu--inline li li > header-menu > details.header__active-menu-item span,
  #Header-{{ section.id }} .mega-menu[open] > summary > span {
    color: {{ section.settings.text_hover_color }};
    text-decoration: none;
  }

  @media screen and (min-width: 750px) {
    #Header-{{ section.id }} .list-menu--inline > li details summary,
    #Header-{{ section.id }} .list-menu--inline li li details,
    #Header-{{ section.id }} ul:not(.mega-menu__sublist,.menu-drawer__menu) li:not(.header__active-menu-item ) > a {
      border-bottom: 0.3rem solid transparent;
      transition: .5s;
    }

    #Header-{{ section.id }} .list-menu--inline > li > header-menu > details > summary {
      padding-right: 2.5rem;
    } 

    #Header-{{ section.id }} .list-menu--inline > li > header-menu > details > summary > svg {
      /*display: none;*/
    } 

    #Header-{{ section.id }} .list-menu--inline > li > header-menu > details > summary:hover, 
    #Header-{{ section.id }} .list-menu--inline li li:active > header-menu > details, 
    #Header-{{ section.id }} .list-menu--inline li li:hover > header-menu >  details,
    #Header-{{ section.id }} .list-menu--inline li li > header-menu > details.header__active-menu-item,
    #Header-{{ section.id }} .list-menu--inline li.header__active-menu-item,
    #Header-{{ section.id }} ul:not(.mega-menu__sublist) li:not(.header__active-menu-item):hover > a {
      border-bottom: 0.3rem solid {{ section.settings.text_hover_color }};
    }

    #Header-{{ section.id }} .list-menu--inline li li .header__active-menu-item {
      text-decoration: none;
    }
  }
  .section-header {
    margin-bottom: {{ section.settings.margin_bottom | times: 0.75 | round: 0 }}px;
    background: {{ section.settings.color_background }};
  }

  @media screen and (min-width: 750px) {
    .section-header {
      margin-bottom: {{ section.settings.margin_bottom }}px;
    }
  }
  @media screen and (max-width: 989px) { 
    /*#Header-{{ section.id }} .cart-count-bubble {
      background: {{ section.settings.text_hover_color }};
      color: {{ section.settings.color_background }};
    }*/

    .section-header.menu-open,
    .section-header.menu-open .header-wrapper,
    .section-header.menu-open #Header-{{ section.id }} {
      background: {{ section.settings.mobile_color_background }};
    }

    #Header-{{ section.id }} .menu-drawer,
    #Header-{{ section.id }} .menu-drawer__navigation,
    #Header-{{ section.id }} details[open].menu-opening > .menu-drawer__submenu,
    #Header-{{ section.id }} .menu-drawer__featured-items-wrap {
      background: {{ section.settings.mobile_color_background }};
    }

    #Header-{{ section.id }} li a,
    #Header-{{ section.id }} .menu-drawer__featured-items-wrap .menu-drawer__featured_title,
    #Header-{{ section.id }} .menu-drawer__featured-items .mega-menu_item a  {
      color: {{ section.settings.mobile_text_color }};
      background: {{ section.settings.mobile_color_background }};
    }

    #Header-{{ section.id }} li a:hover {
      color: {{ section.settings.mobile_text_hover_color }};
    }
    #Header-{{ section.id }} .menu-drawer__menu-item--active, #Header-{{ section.id }} .menu-drawer__menu-item:focus,  #Header-{{ section.id }} .menu-drawer__menu-item[aria-expanded="true"],
    #Header-{{ section.id }} .menu-drawer__close-button:focus, #Header-{{ section.id }} .menu-drawer__menu-item:hover,
    #Header-{{ section.id }} .menu-drawer__close-button:hover {
      color: {{ section.settings.mobile_text_hover_color }};
      background: {{ section.settings.mobile_color_background }};
    }

    #Header-{{ section.id }}  .menu-drawer__utility-links li a,
    .menu-drawer__utility-links {
      background: transparent;
    }
  }
{%- endstyle -%}

