{% style %}

@font-face {
    font-family: 'Double Porter P5';
    src: url('https://cdn.shopify.com/s/files/1/0651/0664/1148/files/Double_Porter_P5.woff?v=1656410026') format('woff');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}

@font-face {
    font-family: 'Double-Porter-fallback';
    size-adjust: 55.38%;
    line-gap-override: 1.2;
    ascent-override: 99%;
    src: local("Verdana");
}


:root {
    --font-family-big-title-3d: 'Double Porter P5', 'Double-Porter-fallback', sans-serif;
    --font-family-big-title: 'Double Porter P5', 'Double-Porter-fallback', sans-serif;
}

{% endstyle %}